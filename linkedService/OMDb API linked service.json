{
	"name": "OMDb API linked service",
	"properties": {
		"description": "Linked service for OMDb API based of custom key",
		"parameters": {
			"baseUrl": {
				"type": "string",
				"defaultValue": "http://www.omdbapi.com/?apikey=f25e6e5b"
			},
			"t": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "@{concat(linkedService().baseUrl, '?', linkedService().t)}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}