{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFBartiz57558301"
		},
		"ABS_linked_service_bartiz_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_linked_service_bartiz'"
		},
		"AzureSQLDataWarehouse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQLDataWarehouse'"
		},
		"AzureSQLDataWarehouse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuresqlserver575583.database.windows.net"
		},
		"AzureSQLDataWarehouse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SQL_DWH"
		},
		"AzureSQLDataWarehouse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"OMDb API linked service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://www.omdbapi.com/?i=tt3896198&apikey=f25e6e5b"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFBartiz57558301_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABS_linked_service_bartiz_connectionString": {
						"value": "[parameters('ABS_linked_service_bartiz_connectionString')]"
					},
					"AzureSQLDataWarehouse_password": {
						"value": "[parameters('AzureSQLDataWarehouse_password')]"
					},
					"AzureSQLDataWarehouse_properties_typeProperties_server": {
						"value": "[parameters('AzureSQLDataWarehouse_properties_typeProperties_server')]"
					},
					"AzureSQLDataWarehouse_properties_typeProperties_database": {
						"value": "[parameters('AzureSQLDataWarehouse_properties_typeProperties_database')]"
					},
					"AzureSQLDataWarehouse_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQLDataWarehouse_properties_typeProperties_userName')]"
					},
					"OMDb API linked service_properties_typeProperties_url": {
						"value": "[parameters('OMDb API linked service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}